{
  "id": "PICA",
  "name": "PicaACG",
  "api_uri": "https://picaapi.picacomic.com",
  "login_uri": "/auth/sign-in",
  "login_method": "post",
  "login_data": {
    "email": "$user_name",
    "password": "$password"
  },
  "profile_uri": "/users/profile",
  "profile_method": "get",
  "encryption": "mixed",
  "encryption_way": [
    {
      "way": "#stringMerge",
      "values": [
        "*uri",
        "*time",
        "*nonce",
        "$method",
        "*apiKey"
      ]
    },
    {
      "way": "#lowerCase"
    },
    {
      "way": "#HMAC-SHA256",
      "secret": "~d}$Q7$eIni=V)9\\RK/P.RM4;9[7|@/CA}b~OW!3?EV`:<>M7pddUBL5n|0/*Cn"
    }
  ],
  "header": {
    "api-key": "*apiKey",
    "accept": "application/vnd.picacomic.com.v1+json",
    "app-channel": "*channel",
    "authorization": "$token",
    "time": "*time",
    "nonce": "*nonce",
    "app-version": "2.2.1.3.3.4",
    "app-uuid": "defaultUuid",
    "image-quality": "*quality",
    "app-platform": "android",
    "app-build-version": "45",
    "Content-Type": "application/json; charset=UTF-8",
    "user-agent": "okhttp/3.8.1",
    "version": "v1.4.1",
    "Host": "picaapi.picacomic.com",
    "signature": "@encryption"
  },
  "account": [
    "user_name",
    "password"
  ],
  "settings": {
    "quality": {
      "name": "Image Quality",
      "default": "original"
    },
    "channel": {
      "name": "App Channel",
      "default": "3"
    }
  },
  "extra": {
    "apiKey": "C69BAF41DA5ABD1FFEDC6D2FEA56B",
    "nonce": [
      {
        "way": "#input",
        "value" : "&uuid1"
      },
      {
        "way": "#replaceAll",
        "keys": [
          "-",
          ""
        ]
      }
    ],
    "uri": [
      {
        "way": "#input",
        "value" : "$uri"
      },
      {
        "way": "#replaceAll",
        "keys": [
          "https://picaapi.picacomic.com/",
          ""
        ]
      }
    ],
    "time": [
      {
        "way": "#input",
        "value": "&time"
      },
      {
        "way": "#~/",
        "key": 1000
      }
    ],
    "channel": [
      "1",
      "2",
      "3"
    ],
    "quality": [
      "original",
      "low",
      "middle",
      "high"
    ]
  }
}
